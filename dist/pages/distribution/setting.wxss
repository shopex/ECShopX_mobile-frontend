.page-distribution-setting {
  line-height: 1;
  background: #f5f5f5;
  color: #000;
  font-size: 28rpx;
}
.page-distribution-setting .container {
  font-size: 28rpx;
}
.page-distribution-setting .wrap {
  padding: 0 30rpx;
}
.page-distribution-setting .title {
  padding-top: 40rpx;
  padding-bottom: 20rpx;
  color: #6d6d6d;
}
.page-distribution-setting .body {
  border-bottom: 2rpx solid #ddd;
}
.page-distribution-setting .input-wrap {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.page-distribution-setting .input-content {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
      -ms-flex: 1;
          flex: 1;
}
.page-distribution-setting .input-edit {
  color: #6d6d6d;
  margin-left: 10rpx;
}
.page-distribution-setting .head-logo {
  width: 160rpx;
  height: 160rpx;
  position: relative;
  margin: 0 auto;
  background: #dfdfdf;
  display: block;
}
.page-distribution-setting .head-logo-icon {
  font-size: 60rpx;
  position: absolute;
  right: 0;
  bottom: 0;
  color: #68cf11;
}
.page-distribution-setting .head-logo-img {
  width: 100%;
  height: 100%;
}
.page-distribution-setting .setting-desc {
  display: -webkit-box;
  -webkit-box-align: center;
}
.page-distribution-setting .setting-item {
  display: -webkit-box;
  -webkit-box-align: center;
  -webkit-box-pack: center;
  -webkit-box-orient: vertical;
}
.page-distribution-setting .setting-item_symbol {
  padding: 0 30rpx;
}
.page-distribution-setting .setting-item-title {
  font-size: 40rpx;
}
.page-distribution-setting .setting-item-body {
  color: #6d6d6d;
  padding: 10rpx 0 20rpx;
}