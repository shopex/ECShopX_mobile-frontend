
.page-distribution-withdraw .withdraw .withdraw-title {
  height: 80rpx;
  display: -webkit-box;
  -webkit-box-align: center;
  padding: 0 30rpx;
  border-bottom: 1rpx solid #ddd;
  margin-bottom: 110rpx;
  color: #333333;
}
.page-distribution-withdraw .withdraw .withdraw-body {
  display: -webkit-box;
  -webkit-box-align: center;
  -webkit-box-pack: justify;
  padding: 0 30rpx 20rpx;
}
.page-distribution-withdraw .withdraw .withdraw-body-input {
  -webkit-box-flex: 1;
  font-size: 32rpx;
}
.page-distribution-withdraw .withdraw .withdraw-body-btn {
  margin-left: 20rpx;
}
.page-distribution-withdraw .g-button._off {
  background: #ddd;
}
.page-distribution-withdraw .g-list-wrap.with-border {
  position: relative;
}
.page-distribution-withdraw .g-list-wrap.with-border::after {
  position: absolute;
  left: 30rpx;
  right: 0;
  bottom: -1rpx;
  height: 1rpx;
  background: #f8f8f8;
  content: " ";
}

.pay-type-picker {
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  z-index: 999;
}

.g-ul {
  padding: 0 30rpx;
  font-size: 28rpx;
  color: #999;
  line-height: 48rpx;
}
.g-ul .g-ul-li {
  display: -webkit-box;
  -webkit-box-align: center;
}
.g-ul .g-ul-li:before {
  content: "";
  display: block;
  background: #999;
  width: 8rpx;
  height: 8rpx;
  border-radius: 50%;
  margin-right: 12rpx;
}