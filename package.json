{
  "name": "ecshopx-vshop",
  "app_name": "ecshopx",
  "version": "2.4.4",
  "private": true,
  "description": "taro app for bbc",
  "scripts": {
    "clean": "rm -rf node_modules/.cache",
    "build:weapp": "taro build --type weapp --trace-warnings",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:weapp:modules": "TARO_MODULES=true npm run build:weapp -- --watch",
    "dev:weapp:prd": "cross-env BUILD_ENV=prd npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "release:h5": "RELEASE=h5 npm run build:h5",
    "lint": "prettier-eslint 'src/**/*.{js,jsx}' --write"
  },
  "author": "",
  "license": "MIT",
  "browsers": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@reduxjs/toolkit": "^1.6.2",
    "@tarojs/cli": "3.3.14",
    "@tarojs/components": "3.3.14",
    "@tarojs/react": "3.3.14",
    "@tarojs/runtime": "3.3.14",
    "@tarojs/taro": "3.3.14",
    "axios": "^0.19.0",
    "azure-storage": "^2.10.3",
    "babel-preset-taro": "3.3.14",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.0.8",
    "dayjs": "^1.10.6",
    "lodash": "^4.17.11",
    "qiniu-js": "^2.5.4",
    "qs": "6.6.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-weapp-storage": "^1.2.0",
    "sr-sdk-wxapp": "^1.3.12",
    "stylenames": "^1.1.6",
    "taro-ui": "^3.0.0-alpha.3",
    "use-immer": "^0.6.0"
  },
  "devDependencies": {
    "@shopex/taro-plugin-modules": "^1.0.11",
    "@tarojs/cli": "3.3.14",
    "@tarojs/mini-runner": "3.3.14",
    "@tarojs/webpack-runner": "3.3.14",
    "@types/react": "^16.8.5",
    "@types/webpack-env": "^1.13.6",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-taro": "3.3.14",
    "chalk": "^4.1.2",
    "cross-env": "^5.2.0",
    "dotenv-flow": "^3.1.0",
    "eslint": "^4.19.1",
    "eslint-config-taro": "3.3.14",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-taro": "3.3.14",
    "fs-extra": "^8.0.1",
    "glob": "^7.1.4",
    "lint-staged": "^12.1.5",
    "lodash-webpack-plugin": "^0.11.5",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-s3-plugin": "^1.2.0-rc.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,scss}": [
      "prettier-eslint --write"
    ]
  }
}
